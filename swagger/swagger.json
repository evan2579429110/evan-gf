                },
                "id": {
                    "description": "用户ID",
                    "type": "integer"
                },
                "nickname": {
                    "description": "用户昵称",
                    "type": "string"
                },
                "passport": {
                    "description": "用户账号",
                    "type": "string"
                },
                "password": {
                    "description": "用户密码",
                    "type": "string"
                },
                "updateAt": {
                    "description": "更新时间",
                    "type": "string"
                }
            }
        },
        "model.UserApiSignUpReq": {
            "type": "object",
            "properties": {
                "nickname": {
                    "type": "string"
                },
                "passport": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
